{
  "name": "prisma-fastify-nexus-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e ./dotenv/dev.env -- ts-node --transpile-only --files src/index.ts",
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node-dev --respawn --transpile-only --files src/index.ts",
    "prod": "node dist/index",
    "dev:pretty": "npm run dev | pino-pretty",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "build": "yarn clean && yarn generate && tsc",
    "generate": "yarn  generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "db:push": "prisma db push --preview-feature",
    "migrate:dev": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio",
    "migrate:deploy": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ./dotenv/.env -- prisma migrate reset",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand"
  },
  "dependencies": {
    "@fastify/view": "^7.4.1",
    "@prisma/client": "4.9.0",
    "@sentry/node": "7.34.0",
    "@sentry/tracing": "7.34.0",
    "altair-fastify-plugin": "5.0.9",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.8",
    "fastify": "4.12.0",
    "fastify-plugin": "4.5.0",
    "graphql": "16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "1.20.1",
    "graphql-upload": "^15.0.2",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^9.0.0",
    "mercurius": "11.5.0",
    "nexus": "1.3.0",
    "nexus-validation-plugin": "^0.2.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@dooboo/eslint-config-node": "^1.0.4",
    "@types/babel__core": "^7.20.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.11.18",
    "dotenv": "16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "29.4.1",
    "prettier": "^2.8.3",
    "prisma": "4.9.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
