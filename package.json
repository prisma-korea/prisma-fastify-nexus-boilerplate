{
  "name": "prisma-fastify-nexus-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e ./dotenv/dev.env -- ts-node --transpile-only --files src/index.ts",
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node-dev --respawn --transpile-only --files src/index.ts",
    "prod": "node dist/index",
    "dev:pretty": "npm run dev | pino-pretty",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "build": "yarn clean && yarn generate && tsc",
    "generate": "yarn  generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "db:push": "prisma db push --preview-feature",
    "migrate:dev": "prisma migrate dev",
    "studio": "dotenv -e ./dotenv/.env prisma studio",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "4.1.0",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "altair-fastify-plugin": "4.5.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.8",
    "fastify": "4.2.1",
    "fastify-plugin": "4.0.0",
    "graphql": "16.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "1.17.0",
    "graphql-upload": "^15.0.2",
    "graphql-ws": "^5.9.1",
    "jsonwebtoken": "^8.5.1",
    "mercurius": "10.1.0",
    "nexus": "1.3.0",
    "nexus-validation-plugin": "^0.2.0",
    "point-of-view": "^6.3.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@dooboo/eslint-config": "^1.4.0",
    "@types/babel__core": "^7.1.19",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "18.0.6",
    "dotenv": "16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.20.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "28.1.3",
    "prettier": "^2.7.1",
    "prisma": "4.1.0",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "packageManager": "yarn@3.2.1"
}
