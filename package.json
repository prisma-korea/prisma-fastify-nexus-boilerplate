{
  "name": "prisma-fastify-nexus-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e ./dotenv/dev.env -- ts-node --transpile-only --files src/index.ts",
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node-dev --respawn --transpile-only --files src/index.ts",
    "prod": "node dist/index",
    "dev:pretty": "npm run dev | pino-pretty",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "build": "pnpm clean && pnpm generate && tsc",
    "generate": "pnpm  generate:prisma && pnpm generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "db:push": "prisma db push --preview-feature",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "3.14.0",
    "@sentry/node": "7.0.0",
    "@sentry/tracing": "7.0.0",
    "altair-fastify-plugin": "4.4.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.8",
    "fastify": "3.29.0",
    "fastify-plugin": "3.0.1",
    "graphql": "16.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "1.17.0",
    "graphql-upload": "^15.0.1",
    "graphql-ws": "^5.8.2",
    "jsonwebtoken": "^8.5.1",
    "mercurius": "9.8.0",
    "nexus": "1.3.0",
    "nexus-validation-plugin": "^0.2.0",
    "point-of-view": "^4.0.0",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@dooboo/eslint-config": "^1.3.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.40",
    "dotenv": "16.0.1",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.17.0",
    "jest": "28.1.0",
    "prettier": "^2.6.2",
    "prisma": "3.14.0",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.3"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
