{
  "name": "prisma-fastify-nexus-boilerplate",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index",
    "dev:pretty": "npm run dev | pino-pretty",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "pnpm clean && pnpm generate && tsc",
    "generate": "pnpm  generate:prisma && pnpm generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "db:push": "prisma db push --preview-feature",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "test": "jest"
  },
  "dependencies": {
    "@autotelic/fastify-opentelemetry": "0.16.0",
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/context-async-hooks": "1.2.0",
    "@opentelemetry/core": "1.2.0",
    "@opentelemetry/exporter-collector": "0.20.0",
    "@opentelemetry/exporter-jaeger": "1.2.0",
    "@opentelemetry/instrumentation-graphql": "0.27.4",
    "@opentelemetry/node": "0.20.0",
    "@opentelemetry/resources": "1.2.0",
    "@opentelemetry/tracing": "0.20.0",
    "@prisma/client": "3.13.0",
    "@sentry/node": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "altair-fastify-plugin": "4.4.1",
    "dotenv": "16.0.0",
    "fastify": "3.29.0",
    "fastify-plugin": "3.0.1",
    "graphql": "16.4.0",
    "graphql-scalars": "1.17.0",
    "mercurius": "9.5.0",
    "nexus": "1.3.0",
    "nexus-prisma": "0.35.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "jest": "28.0.3",
    "prisma": "3.13.0",
    "ts-jest": "28.0.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
